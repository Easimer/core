extern sinf(x : real) : real;
extern cosf(x : real) : real;
extern print(f : real) : real;
extern printbool(b : bool) : bool;

extern eq(l : real, r : real) : bool;
extern neq(l : real, r : real) : bool;
extern lnot(b : bool) : bool;

fn fib(n : real) : real {
	if(eq(n, 0)) then return(0.0);
	if(eq(n, 1)) then return(1.0);
	return(fib(n - 2.0) + fib(n - 1.0));
}

fn Main() : bool {
	print(fib(10));
	print(cosf(0));
	return(true);
}

